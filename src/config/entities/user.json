{
  "name": "user",
  "url": "/user",
  "icon": "people",
  "api": {
    "url": "http://localhost:3000",
    "endpoints": {
      "fetchAll": {
        "url": "users"
      },
      "fetchOne": {
        "url": "users/:id"
      },
      "create": {
        "url": "users"
      },
      "update": {
        "url": "users/:id"
      },
      "delete": {
        "url": "users/:id"
      }
    }
  },
  "schema": [
    "name",
    "email",
    {
      "key": "username",
      "type": "input",
      "inputType": "text",
      "label": "Pseudo",
      "model": "username",
      "placeholder": "Entrez un pseudo"
    },
    {
      "key": "phone",
      "type": "phone-input",
      "label": "Téléphone",
      "model": "phone",
      "placeholder": "Entrez votre téléphone"
    },
    {
      "key": "website",
      "type": "input",
      "inputType": "text",
      "label": "Site web",
      "model": "website",
      "placeholder": "Entrez l'url de votre site"
    }
  ]
}